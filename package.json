{
  "name": "graphql-rest-proxy",
  "version": "0.0.1",
  "scripts": {
    "dev:start": "nodemon",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "upload-spec": "apibuilder upload ben graphql-rest-proxy-config config.spec.json --version $(sem-info tag latest)"
  },
  "dependencies": {
    "apollo-server": "^2.1.0",
    "apollo-server-express": "^2.1.0",
    "got": "^8.3.2",
    "graphql": "^14.0.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.2.1",
    "jsonpath": "^1.0.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/got": "^8.3.4",
    "@types/graphql": "^0.13.4",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/graphql-type-json": "^0.1.3",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.12.0",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^2.9.2"
  },
  "nodemonConfig": {
    "exec": "ts-node src/index.ts config.json overrides.json",
    "watch": [
      "src",
      "config.json",
      "overrides.json"
    ],
    "ext": "ts"
  }
}
