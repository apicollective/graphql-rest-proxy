{
  "name": "graphql-rest-proxy",
  "version": "0.0.1",
  "scripts": {
    "dev:start": "nodemon",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "upload-spec": "apibuilder upload flow graphql-rest-proxy-config config.spec.json --version $(sem-info tag latest)"
  },
  "dependencies": {
    "apollo-server": "^2.14.2",
    "apollo-server-express": "^2.9.6",
    "cors": "^2.8.5",
    "got": "^9.3.2",
    "graphql": "^14.5.8",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.2.1",
    "jsonpath": "^1.0.2",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/got": "^9.6.7",
    "@types/graphql-iso-date": "^3.3.3",
    "@types/graphql-type-json": "^0.1.3",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.144",
    "@types/node": "^10.14.21",
    "nodemon": "^1.19.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.4"
  },
  "nodemonConfig": {
    "exec": "ts-node src/index.ts ../config.json ../overrides.json",
    "watch": [
      "src",
      "../config.json",
      "../overrides.json"
    ],
    "ext": "ts"
  }
}
